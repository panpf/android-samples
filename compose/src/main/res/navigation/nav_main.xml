<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.github.panpf.android.compose.samples.ui.HomeFragment">
        <action
            android:id="@+id/action_home_widgetsFragment"
            app:destination="@id/widgetsFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />

        <action
            android:id="@+id/action_home_layoutsFragment"
            app:destination="@id/layoutsFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
    </fragment>

    <fragment
        android:id="@+id/widgetsFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.WidgetsFragment">
        <action
            android:id="@+id/action_widgets_textFragment"
            app:destination="@id/textFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_buttonFragment"
            app:destination="@id/buttonFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_iconButtonFragment"
            app:destination="@id/iconButtonFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_iconToggleButtonFragment"
            app:destination="@id/iconToggleButtonFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_floatingActionButtonFragment"
            app:destination="@id/floatingActionButtonFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_textFieldFragment"
            app:destination="@id/textFieldFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_asyncImageFragment"
            app:destination="@id/asyncImageFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_checkboxFragment"
            app:destination="@id/checkboxFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_iconFragment"
            app:destination="@id/iconFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_imageFragment"
            app:destination="@id/imageFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_radioButtonFragment"
            app:destination="@id/radioButtonFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_sliderBarFragment"
            app:destination="@id/sliderBarFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
        <action
            android:id="@+id/action_widgets_switchFragment"
            app:destination="@id/switchFragment"
            app:enterAnim="@anim/nav_enter"
            app:exitAnim="@anim/nav_exit"
            app:popEnterAnim="@anim/nav_pop_enter"
            app:popExitAnim="@anim/nav_pop_exit" />
    </fragment>

    <fragment
        android:id="@+id/layoutsFragment"
        android:name="com.github.panpf.android.compose.samples.ui.layouts.LayoutsFragment" />

    <fragment
        android:id="@+id/textFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.text.TextFragment" />

    <fragment
        android:id="@+id/buttonFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.button.ButtonFragment" />

    <fragment
        android:id="@+id/iconButtonFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.iconbutton.IconButtonFragment" />

    <fragment
        android:id="@+id/iconToggleButtonFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.icontogglebutton.IconToggleButtonFragment" />

    <fragment
        android:id="@+id/floatingActionButtonFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.floatingactionbutton.FloatingActionButtonFragment" />

    <fragment
        android:id="@+id/textFieldFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.textfield.TextFieldFragment" />

    <fragment
        android:id="@+id/asyncImageFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.asyncimage.AsyncImageFragment" />

    <fragment
        android:id="@+id/checkboxFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.checkbox.CheckboxFragment" />

    <fragment
        android:id="@+id/iconFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.icon.IconFragment" />

    <fragment
        android:id="@+id/imageFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.image.ImageFragment" />

    <fragment
        android:id="@+id/radioButtonFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.radiobutton.RadioButtonFragment" />

    <fragment
        android:id="@+id/sliderBarFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.sliderbar.SliderBarFragment" />

    <fragment
        android:id="@+id/switchFragment"
        android:name="com.github.panpf.android.compose.samples.ui.widgets.switch.SwitchFragment" />
</navigation>